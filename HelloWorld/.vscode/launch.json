    "version": "0.2.0",
    "configurations": [
        {
            "name": "Cortex Debug",
            "cwd": "${workspaceRoot}/build",
            "executable": "${command:cmake.launchTargetPath}",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "external",
            "gdbPath": "/usr/bin/gdb-multiarch",
            "gdbTarget": "192.168.10.100:3333",
            "showDevDebugOutput": "raw"
        }
    ]
}